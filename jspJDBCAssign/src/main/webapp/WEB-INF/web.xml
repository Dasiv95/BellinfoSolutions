<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
  <param-name>network-name</param-name>
  <param-value>sprint</param-value>
  </context-param>
  <servlet>
  <servlet-name>teleRegister</servlet-name>
  <servlet-class>telephoneServlet.telephoneRegisterServlet</servlet-class>
  <init-param>
  <param-name>Reg_Contact</param-name>
  <param-value>888-888-8888</param-value>
  </init-param>
  </servlet>
  <servlet>
  <servlet-name>teleLogin</servlet-name>
  <servlet-class>telephoneServlet.teleLoginServlet</servlet-class>
  <init-param>
  <param-name>Log_Contact</param-name>
  <param-value>888-888-8881</param-value>
  </init-param>
  </servlet>
  <servlet>
  <servlet-name>teleHome</servlet-name>
  <servlet-class>telephoneServlet.teleHomeServlet</servlet-class>
  <init-param>
  <param-name>Home_Contact</param-name>
  <param-value>888-888-8882</param-value>
  </init-param>
  </servlet>
  <servlet>
  <servlet-name>teleBills</servlet-name>
  <servlet-class>telephoneServlet.teleBillsServlet</servlet-class>
  <init-param>
  <param-name>Bill_Contact</param-name>
  <param-value>888-888-8883</param-value>
  </init-param>
  </servlet>
  <servlet>
  <servlet-name>telePay</servlet-name>
  <servlet-class>telephoneServlet.telePayServlet</servlet-class>
  <init-param>
  <param-name>Pay_Contact</param-name>
  <param-value>888-888-8884</param-value>
  </init-param>
  </servlet>
  
  <servlet-mapping>
     <servlet-name>teleRegister</servlet-name>
     <url-pattern>/reg.do</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
     <servlet-name>teleLogin</servlet-name>
     <url-pattern>/log.do</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
     <servlet-name>teleHome</servlet-name>
     <url-pattern>/home.do</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
     <servlet-name>teleBills</servlet-name>
     <url-pattern>/bill.do</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
     <servlet-name>telePay</servlet-name>
     <url-pattern>/pay.do</url-pattern>
   </servlet-mapping>
    
  
</web-app>
